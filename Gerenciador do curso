//classe de gerenciamento
public class GerenciaCurso {
//objeto Curso
Curso cs = new Curso();
//metodos do objeto Curso gerenciado pela classe
public void CadastrarCurso(){
	cs.CadastrarNome();
	cs.CadastrarDuracao();
	cs.CadastrarPreco();
	cs.CadastrarTelefone();
	cs.CadastrarEmail();
	cs.CadastrarEndereco();
	cs.CadastrarAlunos();
	cs.CadastrarProfessor();
	cs.CadastrarCoordenacao();
	cs.CadastrarDirecao();
}
public void MostrarCurso(){
	cs.MostrarNome();
	cs.MostrarDuracao();
	cs.MostrarPreco();
	cs.MostrarTelefone();
	cs.MostrarEmail();
	cs.MostrarEndereco();
	cs.MostrarAlunos();
	cs.MostrarProfessor();
	cs.MostrarCoordenacao();
	cs.MostrarDirecao();	
}
}
